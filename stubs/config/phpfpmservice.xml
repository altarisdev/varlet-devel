<service>
  <id>VarletPHP</id>
  <name>Varlet PHP-FPM</name>
  <description>Varlet PHP-FPM</description>
  <executable><<INSTALL_DIR>>\php73\php-cgi.exe</executable>
  <arguments>-b 127.0.0.1:9001</arguments>
  <logpath><<INSTALL_DIR>>\tmp\</logpath>
  <logmode>reset</logmode>
  <startmode>Manual</startmode>
  <onfailure action="restart"/>
</service>
